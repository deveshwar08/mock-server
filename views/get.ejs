<%- include('partials/header.ejs') %>
 <div class="container no-flex" >
     <div class="get-all">
         <form id='get-all-form'>
             <div class="form-group">
                 <label for='servername'>Server Name: </label>
                 <input type='text' name='servername' class='form-control' required/>
             </div>
            <div class="form-group">
                <label for='modelname'>Model Name: </label>
                <input type='text' name='modelname' class='form-control' required/>
            </div>
            <div class="form-group">
                <button type='submit'>Get all</button>
            </div>
         </form>
     </div>
     
     <div class="get-id">
        <form id='get-id-form'>
            <div class="form-group">
                <label for='servername'>Server Name: </label>
                <input type='text' name='servername' class='form-control' required/>
            </div>
           <div class="form-group">
               <label for='modelname'>Model Name: </label>
               <input type='text' name='modelname' class='form-control' required/>
           </div>
           <div class="form-group">
                <label for='id'>Id: </label>
                <input type='text' name='id' class='form-control' required/>
            </div>
           <div class="form-group">
               <button type='submit'>Get Id</button>
           </div>
        </form>
     </div>

     <div>
         <h3>Search results</h3>
         <div class="display"></div>

    </div>
 </div>
 <script src='fe2.js' type='text/javascript'></script>
<%- include('partials/footer.ejs') %> 